{
  "homepage": ".",
  "name": "react-typescript-starter-pack",
  "version": "0.1.0",
  "type": "module",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.5.1",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "bulma": "^0.9.4",
    "classnames": "^2.3.2",
    "cors": "^2.8.5",
    "express": "^4.19.2",
    "joi": "^17.13.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.23.1",
    "react-router-dom": "^6.23.1",
    "react-scripts": "5.0.1",
    "sequelize": "^6.37.3",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "typescript": "^4.6.3",
    "typescript-eslint-parser": "^22.0.0",
    "uuid": "^9.0.1",
    "web-vitals": "^1.1.1",
    "ws": "^8.17.0"
  },
  "scripts": {
    "format": "prettier --write './src/**/*.{ts,tsx}'",
    "lint": "npm run format && eslint ./src --fix",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@mate-academy/eslint-config-react-typescript": "latest",
    "@mate-academy/students-ts-config": "latest",
    "@types/node": "^16.18.80",
    "@types/react": "^18.2.55",
    "@types/react-dom": "^18.2.19",
    "@types/uuid": "^9.0.8",
    "autoprefixer": "^10.4.19",
    "eslint": "^7.32.0",
    "gh-pages": "^6.1.1",
    "nodemon": "^3.1.3",
    "postcss": "^8.4.38",
    "prettier": "^3.2.5",
    "sass": "^1.72.0",
    "sequelize-cli": "^6.6.2",
    "tailwindcss": "^3.4.3"
  },
  "compilerOptions": {
    "target": "ES5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "tailwind.config.js",
    "postcss.config.js",
    "public/API/server.js",
    "./models/index.js"
  ]
}
